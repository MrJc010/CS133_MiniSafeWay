<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nguye.minisafeway">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Welcome"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FoodList" />
        <activity android:name=".FoodDetail" />
        <activity android:name=".Signup" />
        <activity android:name=".Home" />
        <activity android:name=".Cart" />
        <activity android:name=".HistoryList" />
        <activity
            android:name=".Guest"
            android:label="@string/title_activity_guest"
            android:theme="@style/AppTheme" />
        <activity android:name=".Signin" />
        <activity android:name=".WishList" />
        <activity android:name=".Aboutus"></activity>
    </application>

</manifest>